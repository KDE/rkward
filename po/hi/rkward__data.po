#: rkward/plugins/data/subset_dataframe.rkh:63
#, kde-format
msgid "<link href=\"rkward://rhelp/subset\"/>"
msgstr "<link href=\"rkward://rhelp/subset\"/>"

#. i18n: ectx: (Subset of data) <dialog label="...">
#: rkward/plugins/data/subset_dataframe.xml:73
#, kde-format
msgid "Subset of data"
msgstr "डेटा का उपसमूह"

#. i18n: ectx: (Subset of data) <dialog label="Subset of data"> <varselector label="...">
#: rkward/plugins/data/subset_dataframe.xml:75
#, kde-format
msgid "Select data"
msgstr "डेटा चुनें"

