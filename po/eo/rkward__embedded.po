#: rkward/plugins/distributions/../plots/plot_options.rkh:58
#, kde-format
msgid ""
"<i>code.calculate</i>: Currently this contains only the <b>grid</b>-options."
msgstr ""
"<i>code.calculate</i>: Nuntempe ĉi tio enhavas nur la <b>krado</b>-opciojn."

