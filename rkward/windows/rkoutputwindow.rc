<!DOCTYPE kpartgui>
<kpartgui name="rkward" version="0.4.3">
	<MenuBar>
		<Menu name="file">
			<Action name="print_output"/>
		</Menu>
		<Menu name="edit">
			<Action name="copy"/>
			<Separator/>
			<Action name="output_flush"/>
		</Menu>
		<Menu name="view">
			<Separator/>
			<Action name="output_refresh"/>
		</Menu>
	</MenuBar>
	<ToolBar fullWidth="true" name="mainToolBar">
		<Action name="output_flush"/>
		<Action name="output_refresh"/>
	</ToolBar>
</kpartgui>
