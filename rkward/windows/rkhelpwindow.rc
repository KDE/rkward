<!DOCTYPE kpartgui>
<kpartgui name="rkward" version="0.3.4">
	<MenuBar>
		<Menu name="file">
			<Action name="print_help"/>
		</Menu>
		<Menu name="edit">
			<Action name="copy"/>
		</Menu>
		<Menu name="run">
			<Action name="run_selection"/>
		</Menu>
	</MenuBar>
	<ToolBar fullWidth="true" name="navigation_toolbar">
