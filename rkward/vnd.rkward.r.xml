<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="text/r">
    <comment>R source file</comment>
    <comment xml:lang="cs">Zdrojový soubor R</comment>
    <comment xml:lang="nl">R-bronbestand</comment>
    <comment xml:lang="pt">Ficheiro de código em R</comment>
    <comment xml:lang="pt_BR">Arquivo de código do R</comment>
    <comment xml:lang="sk">Zdrojový súbor R</comment>
    <comment xml:lang="uk">файл коду R</comment>
    <acronym>R</acronym>
    <expanded-acronym>R programming language</expanded-acronym>
    <generic-icon name="text-x-generic"/>
    <sub-class-of type="text/plain"/>
    <glob pattern="*.r"/>
    <alias type="text/x-r"/>
  </mime-type>
  <mime-type type="application/rdata">
    <comment>R data/workspace file</comment>
    <comment xml:lang="cs">Soubor pracovní plochy/dat R</comment>
    <comment xml:lang="nl">R-gegevens/werkruimtebestand</comment>
    <comment xml:lang="pt">Ficheiro de dados/área de trabalho do R</comment>
    <comment xml:lang="pt_BR">Arquivo de dados/espaço de trabalho do R</comment>
    <comment xml:lang="sk">Súbor údajov/pracovnej plochy R</comment>
    <comment xml:lang="uk">файл даних/робочого простору R</comment>
    <acronym>RData</acronym>
    <expanded-acronym>R data/workspace file</expanded-acronym>
    <generic-icon name="package-x-generic"/>
    <sub-class-of type="application/x-gzip"/>
    <glob pattern="*.rdata"/>
    <glob pattern="*.rda"/>
    <alias type="application/x-rdata"/>
  </mime-type>
</mime-info>
