            <stretch />
            <frame label="Bootstrap options">
              <spinbox id="spin_samples" label="Number of samples" type="integer" initial="49" min="1" size="small" />
            </frame>
          </column>
        </row>
      </tab>
    </tabbook>
  </dialog>
</document>
